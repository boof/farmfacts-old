<h2>Users</h2>

<table id="cmUsers">
	<thead>
		<tr>
			<th class="cmUserName">Name</th>
			<th class="cmUserEMail">E-Mail</th>
			<th class="cmAction"></th>
		</tr>
	</thead>
	<tbody>
		<tr class="cmAction">
			<td colspan="2"></td>
			<td class="cmAction"><%= link_to image_tag('icons/page_new.gif'), new_admin_user_path %></td>
		</tr>
		<%= render :partial => 'user', :collection => @users %>
		<tr class="cmAction">
			<td colspan="2"></td>
			<td class="cmAction"><%= link_to image_tag('icons/page_new.gif'), new_admin_user_path %></td>
		</tr>
	</tbody>
</table>
